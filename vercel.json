{
  "buildCommand": "echo 'No build needed for static HTML'",
  "outputDirectory": "v1/frontend",
  "rewrites": [
    {
      "source": "/(.*)",
      "destination": "/v1/frontend/$1"
    }
  ],
  "headers": [
    {
      "source": "/(.*)",
      "headers": [
        {
          "key": "Access-Control-Allow-Origin",
          "value": "*"
        }
      ]
    }
  ]
}
